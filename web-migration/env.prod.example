# Configuration de production pour Dyad Web

# Base de donn√©es PostgreSQL
POSTGRES_DB=dyad_web_prod
POSTGRES_USER=dyad_user
POSTGRES_PASSWORD=your-super-secure-database-password

# Authentification
JWT_SECRET=your-super-secret-jwt-key-for-production-change-this

# URLs
FRONTEND_URL=https://dyad-web.vercel.app
VITE_API_URL=https://api.dyad-web.com/api
VITE_WS_URL=wss://api.dyad-web.com

# APIs IA
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Configuration SSL (optionnel)
SSL_CERT_PATH=/etc/nginx/ssl/cert.pem
SSL_KEY_PATH=/etc/nginx/ssl/key.pem

# Monitoring (optionnel)
SENTRY_DSN=your-sentry-dsn
LOG_LEVEL=info
